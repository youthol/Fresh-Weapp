/* pages/result/result.wxss */
.icon-container {
  width: 300rpx;
  height: 300rpx;
  margin: 0 auto;
  padding-top: 100rpx;
}

.icon-item {
  width: 100%;
  height: 100%;
  background-color: #19be6b;
  border-radius: 150rpx;
  position: relative;
}

.success {
  color: aliceblue;
  position: absolute;
  top: 150rpx;
  font-size: 150rpx;
  width: 100%;
  text-align: center;
  margin-top: -75rpx;
}

.msg {
  display: block;
  margin-top: 60rpx;
  width: 100%;
  text-align: center;
}
.desc {
  margin-top: 20rpx;
  font-size: 28rpx;
  color: rgb(151, 161, 165);
}

.image {
  display: block;
  width: 80%;
  margin: 0 auto;
  margin-top: 50rpx;
  padding-bottom: 50rpx;
}